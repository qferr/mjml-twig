{
    "name": "qferr/mjml-twig",
    "description": "Twig extension that provides a filter that processes a mjml email template",
    "type": "library",
    "authors": [
        {
            "name": "Quentin",
            "email": "qferrer@outook.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "twig/twig": "^2.0|^3.0",
        "qferr/mjml-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/"
    },
    "autoload": {
        "psr-4": {
            "Qferrer\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qferrer\\Tests\\" : "tests/"
        }
    }
}
